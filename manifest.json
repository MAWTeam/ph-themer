{
    "manifest_version": 2,
    "name":         "Themes for Product Hunt",
    "description":  "Custom themes for Product Hunt.",
    "version": "0.1.0",
    
    "content_scripts": [ {
        "js": [
            "main.js"
        ],
        "matches":          [   "*://*.producthunt.com/*"
                            ],
        "run_at": "document_end",
        "all_frames": false
    } ],
    
    "web_accessible_resources": [ "data/*" ],
    "icons": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },

    "converted_from_user_script": true
}